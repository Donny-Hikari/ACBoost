
ifeq ($(ACBOOST_ROOT),)
	ACBOOST_ROOT=../..
endif

scriptsdir=$(ACBOOST_ROOT)/scripts
entry=build/main

test: build
	@$(scriptsdir)/test

build: src/*.cpp
	g++ -o $(entry) src/*.cpp --std=c++11

run:
	@./$(entry) < "cases/$(N).in"

diff:
	@bash $(scriptsdir)/compare $(N)

lastnewline:
	@bash $(scriptsdir)/lastnewline
